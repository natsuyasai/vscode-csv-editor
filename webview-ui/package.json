{
  "name": "webview-ui",
  "version": "0.0.2-alpha",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:debug:watch": "tsc && vite build --sourcemap \"inline\" --minify false --mode \"development\" --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "test:story": "vitest --project storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-types": "tsc --noEmit",
    "lint": "eslint ./ && npm run markuplint",
    "markuplint": "markuplint \"./src/**/*.{jsx,tsx}\"",
    "postinstall": "node scripts/copyicon.cjs"
  },
  "dependencies": {
    "@vscode-elements/react-elements": "^2.3.1",
    "@vscode/codicons": "^0.0.40",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "react": "^19.1.1",
    "react-data-grid": "^7.0.0-beta.57",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@markuplint/jsx-parser": "^4.7.21",
    "@markuplint/react-spec": "^4.5.21",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.5",
    "jsdom": "^26.1.0",
    "markuplint": "^4.12.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "storybook": "^9.1.5",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
